{
  "name": "aerocode",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "zero": "npm install && cd backend && npm install && npm run setup && cd .. && concurrently \"npm run backend\" \"npm run frontend\"",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "backend": "node backend/server.js",
    "frontend": "vite",
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "stop": "pkill -f \"node backend/server.js\" && pkill -f \"vite\"",
    "prisma:migrate": "cd backend && prisma migrate dev && cd ..",
    "prisma:seed": "cd backend && node prisma/seed.js && cd ..",
    "prisma:generate": "cd backend && prisma generate && cd .."
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
